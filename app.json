{
    "expo": {
        "name": "therify",
        "description": "An awesome photo-sharing",
        "sdkVersion": "25.0.0",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./app.png",
        "primaryColor": "#00AAFF",
        "splash": {
            "image": "./splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "packagerOpts": {
            "assetExts": ["otf"]
        },
        "androidStatusBar": {
            "backgroundColor": "#ffffff",
            "barStyle": "dark-content"
        }
    }
}
